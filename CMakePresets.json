{
    "version": 3,
    "cmakeMinimumRequired": {
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "x64-Debug",
      "hidden": false,
      "generator": "Ninja",
      "description": "x64 Debug build",
      "binaryDir": "${sourceDir}/Build/${presetName}",
      "toolset": {
        "value": "v143,host=x64",
        "strategy": "external"
      },
      "architecture": {
        "value": "x64",
        "strategy": "external"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_C_COMPILER": "cl",
        "CMAKE_CXX_COMPILER": "cl",
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/Install/${presetName}",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "YES"
      }
    },
    {
      "name": "x64-Release",
      "hidden": false,
      "generator": "Ninja",
      "description": "x64 Release build",
      "binaryDir": "${sourceDir}/Build/${presetName}",
      "toolset": {
        "value": "v143,host=x64",
        "strategy": "external"
      },
      "architecture": {
        "value": "x64",
        "strategy": "external"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_C_COMPILER": "cl",
        "CMAKE_CXX_COMPILER": "cl",
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/Install/${presetName}",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "YES"
      }
    }]}